{
	"name": "prefix-cc",
	"displayName": "prefix.cc",
	"description": "A prefix.cc extension for Semantic Web and Linked Data Developers",
	"version": "1.0.1",
	"publisher": "umutsims",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/sumutcan/vscode-prefixcc"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:prefix-cc.findNamespace"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "prefix-cc.findNamespace",
				"title": "Prefix.cc: Find Namespace"
			}
		],
		"keybindings": {
			"command": "prefix-cc.findNamespace",
			"key": "ctrl+k p",
			"mac": "cmd+k p",
			"when": "editorHasSelection"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"axios": "^0.19.2",
		"@types/axios": "^0.14.0"
	},
	"dependencies": {
		"@types/axios": "^0.14.0",
		"axios": "^0.19.2"
	}
}
